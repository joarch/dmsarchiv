{
  "name": "Standardexport",
  "json_export_datei": "export_documents.json",
  "log_datei": "export.log",
  "suchparameter_list": [
  ],
  "export": {
    "export_format": "xlsx",
    "vorlage_dateiname": "",
    "vorlage_sheet_name": "",
    "dateiname": "export_documents.xlsx",
    "dateiname_postfix": "_%Y-%m-%d",
    "id_feld": "docId",
    "fortlaufendes_feld": "nummer_sorierung",
    "spaltenueberschrift": "ja",
    "spaltenueberschrift_format": {
      "format": "PatternFill",
      "start_color": "AAAAAA",
      "end_color": "AAAAAA",
      "fill_type": "solid"
    },
    "formate": [
      {
        "match": "^Warnung .*",
        "format": {
          "format": "PatternFill",
          "start_color": "FFFF00",
          "end_color": "FFFF00",
          "fill_type": "solid"
        }
      },
      {
        "match": "^Fehler .*",
        "format": {
          "format": "PatternFill",
          "start_color": "FF0000",
          "end_color": "FF0000",
          "fill_type": "solid"
        }
      }
    ],
    "sortierung": {
      "felder": [
        {
          "feld": "nummer_sorierung",
          "wie": "aufsteigend"
        },
        {
          "feld": "docId",
          "wie": "aufsteigend"
        }
      ]
    },
    "spalten": [
      {
        "feld": "docId",
        "ueberschrift": "DocId"
      },
      {
        "feld": "ctimestamp",
        "ueberschrift": "Geändert",
        "number_format": "DD.MM.YYYY"
      },
      {
        "feld": "bemerkung",
        "ueberschrift": "Bemerkung"
      },
      {
        "feld": "dyn_1_1526633932510",
        "ueberschrift": "Nummer",
        "type": "string"
      },
      {
        "feld": "dyn_1_1526633932510",
        "alias": "nummer_sorierung",
        "type": "int",
        "mapping": {
          "type": "re",
          "methode": "sub",
          "argumente": [
            "^18\\.|19\\.|20\\.",
            ""
          ]
        },
        "ueberschrift": "Nummer zur Sortierung"
      },
      {
        "feld": "",
        "alias": "warnung",
        "computed": "'Warnung nicht fortlaufend' if nicht_fortlaufend() else ''",
        "ueberschrift": "Warnung"
      },
      {
        "feld": "folder",
        "ueberschrift": "Ordner",
        "mapping": {
          "typ": "datei",
          "dateiname": "folders.json",
          "id": "oId",
          "name": "foldername"
        }
      },
      {
        "feld": "mainfolder",
        "ueberschrift": "Hauptordner",
        "mapping": {
          "typ": "datei",
          "dateiname": "folders.json",
          "id": "oId",
          "name": "foldername"
        }
      },
      {
        "feld": "docart",
        "ueberschrift": "Dokumentenart",
        "mapping": {
          "typ": "datei",
          "dateiname": "types.json",
          "id": "id",
          "name": "name"
        }
      },
      {
        "feld": "",
        "ueberschrift": "Geprüft"
      }
    ]
  }
}
